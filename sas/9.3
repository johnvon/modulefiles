#%Module######################################################################
#
# SAS modulefile
#
set PRODUCT SAS
set _VERSION 9.3 
set _CURPATH /opt/SAS_$_VERSION/SASFoundation/$_VERSION/

# cannot load multiple sas version simultaneously
conflict sas

proc ModulesHelp { } {
puts stderr "This modulefile sets the default version of $PRODUCT"
puts stderr "to $_VERSION"
}

remove-path PATH /usr/local/bin
remove-path PATH /usr/local/bin

prepend-path PATH $_CURPATH
