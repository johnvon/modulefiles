#%Module######################################################################
#
# texlive modulefile
#
set PRODUCT texlive 
set _VERSION 2014 

proc ModulesHelp { } {
puts stderr "This modulefile s the default version of $PRODUCT"
puts stderr "to $_VERSION"
}

prepend-path PATH /cac/contrib/texlive/2014/bin/x86_64-linux

prepend-path INFOPATH /cac/contrib/texlive/2014/texmf-dist/doc/info 
prepend-path MANPATH /cac/contrib/texlive/2014/texmf-dist/doc/man 

